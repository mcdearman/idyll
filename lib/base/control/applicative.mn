pub class Applicative (t : Type -> Type) where
  pure : a -> t a
  (<*>) : t (a -> b) -> t a -> t b
  liftA2 : (a -> b -> c) -> t a -> t b -> t c
  (*>) : t a -> t b -> t b
  (<*) : t a -> t b -> t a

pub class (Applicative f) => Alternative (f : Type -> Type) where
  empty : f a
  (<|>) : f a -> f a -> f a
  some : f a -> f (List a)
  many : f a -> f (List a)