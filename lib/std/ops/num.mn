@pub
@minimal((+), (*), abs, signum, fromInteger, (negate | (-)))
class Num a where
  (+), (-), (*) : a -> a -> a
  x - y = x + negate y
  negate : a -> a 
  negate = 0 - x
  abs : a -> a
  signum : a -> Int
  fromInteger : Integer -> a
  
@inline(always)
subtract : a -> a -> a
subtract x y = y - x
